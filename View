package View;

import Controller.Controller;


import javax.swing.*;



public class View extends JFrame {
    static int GO = 0;
    public View(String s){
        super(s);
    }
    public void update(){
    this.repaint();
    }
    public static void endGame(){
        Controller.stopGame();

    }
    public static void minusGO(){GO--;}
    public static void plusGO(){GO++;}
}
